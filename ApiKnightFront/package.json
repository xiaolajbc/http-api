{
  "name": "@apiknight/front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://apiknight.github.io",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "lint-strict:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "deploy": "gh-pages -d build",
    "test": "npx playwright test",
    "test:vitest": "vitest",
    "test:vitest-prod": "vitest --coverage",
    "test:debug": "npx playwright test --ui",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@apiknight/monitor-sdk": "workspace:^",
    "@bytemd/react": "^1.21.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ebay/nice-modal-react": "^1.2.13",
    "@emotion/css": "^11.13.5",
    "@playwright/test": "^1.43.1",
    "@types/mockjs": "^1.0.10",
    "@types/serialize-javascript": "^5.0.4",
    "@types/yaml": "^1.9.7",
    "antd": "^5.22.4",
    "browserslist": "^4.24.2",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.462.0",
    "nanoid": "^5.0.9",
    "postcss-loader": "^8.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-resizable-panels": "^2.1.7",
    "react-use-event-hook": "^0.9.6",
    "serialize-javascript": "^6.0.2",
    "use-resize-observer": "^9.1.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15"
  }
}
